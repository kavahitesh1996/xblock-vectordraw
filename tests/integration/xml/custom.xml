              display_name="Custom Exercise"
              description="Custom exercise description"
              width="600"
              height="450"
              axis="false"
              show_navigation="true"
              show_vector_properties="{{ show_vector_properties }}"
              show_slope_for_lines="true"
              add_vector_label="Custom button label"
